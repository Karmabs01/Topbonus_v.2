generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider          = "mysql"
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
}


model otps {
  id         String    @id @db.VarChar(36)
  email      String    @db.VarChar(255)
  code       String    @db.VarChar(6)
  expires_at DateTime  @db.DateTime(0)
  created_at DateTime? @default(now()) @db.DateTime(0)
}

model users {
  login          String?  @db.VarChar(100)
  id             String   @id @db.VarChar(100)
  VIP            String   @db.VarChar(25)
  balance        Decimal? @db.Decimal(6, 2)
  country        String   @db.VarChar(25)
  input          String   @db.VarChar(100)
  password       String   @db.VarChar(25)
  tickets        String?  @db.VarChar(3)
  winbalance     String   @db.VarChar(255)
  customer       String   @db.VarChar(15)
  status_payment String?  @db.Text
  phone_number   String?  @db.VarChar(255)
  spins_waiting  String?  @db.VarChar(3)
  geo_approve    String?  @db.VarChar(10)
  leads          String?  @db.Text
  sales          String?  @db.Text
  qr_code        String?  @db.VarChar(255)
}
model usersE {
  email          String?  @db.VarChar(100)
  id             String   @id @db.VarChar(100)
  VIP            String   @db.VarChar(25)
  balance        Decimal? @db.Decimal(6, 2)
  country        String   @db.VarChar(25)
  input          String   @db.VarChar(100)
  password       String   @db.VarChar(25)
  tickets        String?  @db.VarChar(3)
  winbalance     String   @db.VarChar(255)
  customer       String   @db.VarChar(15)
  status_payment String?  @db.Text
  phone_number   String?  @db.VarChar(255)
  spins_waiting  String?  @db.VarChar(3)
  geo_approve    String?  @db.VarChar(10)
  leads          String?  @db.Text
  sales          String?  @db.Text
  qr_code        String?  @db.VarChar(255)
}


